[Unit]
Description={{ keycloak_service_description }}
After=network.target

[Service]
User=root
Group=root
ExecStart={{ keycloak_bin_path }}/domain.sh -b 0.0.0.0 --host-config=host-{{ keycloak_service_target_config }}.xml -bmanagement {{ keycloak_host_address }}
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target
